        {
          "dataset": "mysql",
          "datasource": {
            "type": "mysql",
            "uid": "${DS_MYSQL}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  IF (\n    has_all_materials = 1\n    AND (storage_capacity - stored) >= out_size,\n    'Running',\n    'Stopped'\n  ) AS Label,\n  COUNT(*) AS value\nFROM\n  (\n    SELECT\n      f.*,\n      COALESCE(SUM(s.quantity * m.size), 0) as `stored`,\n      (\n        SELECT\n          COALESCE(SUM(po.quantity * pm.size), 0)\n        FROM\n          processes po\n          JOIN materials pm ON po.id_material = pm.id_material\n          AND po.id_factory = f.id_factory\n          AND po.type = 'out'\n      ) as `out_size`,\n      IF(\n        COUNT(DISTINCT p.id_material) = SUM(IF(COALESCE(ps.quantity, 0) >= p.quantity, 1, 0)),\n        1,\n        0\n      ) AS has_all_materials\n    FROM\n      factories f\n      LEFT JOIN factory_storage s ON f.id_factory = s.id_factory\n      LEFT JOIN materials m ON m.id_material = s.id_material\n      LEFT JOIN processes p ON (\n        p.id_factory = f.id_factory\n        AND p.type = 'in'\n      )\n      LEFT JOIN factory_storage ps ON (\n        f.id_factory = ps.id_factory\n        AND p.id_material = ps.id_material\n      )\n    GROUP BY\n      f.id_factory\n  ) a\nGROUP BY\n  Label",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Running / Stopped factories",
      "type": "bargauge"
    },
